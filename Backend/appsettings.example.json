{
  "BaseUrl": "http://localhost:8080",
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "ConnectionStrings": {
    "DefaultConnection": "Server=db; User ID=user; Password=password; Database=db"
  },

  "Jwt": {
    "Issuer": "UGHServer",
    "Audience": "MyApp",
    "Key": "YOUR_JWT_KEY_HERE"
  },
  "AllowedHosts": "*",
  "MailSettings": {
    "Mail": "your-email@example.com",
    "DisplayName": "your-display-name@example.com",
    "Password": "YOUR_MAIL_PASSWORD",
    "Host": "your-mail-host.com",
    "Port": 587
  },
  "Facebook": {
    "EMAIL": "your-facebook-email@example.com",
    "PASS": "YOUR_FACEBOOK_PASSWORD",
    "MAIN_GROUP_ID": "YOUR_FACEBOOK_GROUP_ID"
  },
  "TemplateSettings": {
    "SuccessTemplate": "Template/confirmation.html",
    "FailedTemplate": "Template/DeclinedConfirmation.html"
  },
  "AwsOptions": {
    "Profile": "",
    "Region": "eu-north-1",
    "AccessKey": "YOUR_AWS_ACCESS_KEY",
    "SecretKey": "YOUR_AWS_SECRET_KEY",
    "AWS_Url": "https://your-bucket.s3.eu-north-1.amazonaws.com/",
    "API_Key": "",
    "BucketName": "your-bucket-name"
  },
  "FileEndpoints": {
    "FileEndpointPrefix": "/api/FileManagement/file"
  },
  "Stripe": {
    "ApiKey": "YOUR_STRIPE_API_KEY",
    "PaymentSuccessWebhookId": "YOUR_STRIPE_WEBHOOK_ID"
  },
  "StripeWebhookPolicy": {
    "PermitLimit": 3,
    "QueueLimit": 0,
    "WindowMinutes": 1
  },
  "DSGVOSettings": {
    "DeletedUserRetentionDays": 30
  }
} 